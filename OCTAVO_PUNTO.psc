Algoritmo OCTAVO_PUNTO  
	a = 1
   d = 1.0
	
    a <- 46 % 9 + 4 * 4 - 2
    Escribir "a = " concatenado con a
	
    a <- 45 + 43 % 5 * (23 * 3 % 2)
    Escribir "a = " concatenado con a
	
    a <- 45 + 45 * 50 % a
    Escribir "a = " concatenado con a
	
    d <- 1.5 * 3 + (-d)
    Escribir "d = " concatenado con d
	
    d <- 1.5 * 3 + d
    Escribir "d = " concatenado con d
	
    a <- a % (3 / a + 3)
    Escribir "a = " concatenado con a
	
FinAlgoritmo
